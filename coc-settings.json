{
  "tailwindCSS.includeLanguages": {
    "*.ejs": "html"
  },
  "cSpellExt.enableDictionaries": [
    "portuguese-brazilian"
  ],
  "cSpell.language": "en,pt,pt_BR",
  "cSpell.userWords": [
    "Muntz",
    "Neovim",
    "Tasker",
    "Xuyuanp",
    "alphanum",
    "autoload",
    "brunodavi",
    "ctrlp",
    "ctrlpvim",
    "deps",
    "dotfiles",
    "gundo",
    "jreybert",
    "kyazdani",
    "mhinz",
    "mygroup",
    "nvim",
    "regexs",
    "romgrk",
    "syntastic",
    "tpope",
    "trybe",
    "uncommenting",
    "vals",
    "vimagit"
  ],
  "explorer.keyMappings.global": {
    "<cr>": [
      "expandable?",
      ["expanded?", "collapse", "expand"],
      "open"
    ],
    "<space>": ["wait", "expandable?", "cd", "open"]
  },
  "eslint.autoFixOnSave": true,
  "explorer.icon.enableNerdfont": true,
  "explorer.file.showHiddenFiles": true,
  "explorer.debug": true,
  "emmet.extensionsPath": "~/.config/nvim/plugins/coc.nvim/emmet",
  "emmet.excludeLanguages": [],
  "emmet.includeLanguages": {
    "markdown": "html",
    "ejs": "html"
  }
}
